				android:textAlignment="center"
				android:textAppearance="?android:attr/textAppearanceSmall"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center"
				>
				</TextView>
				
			</LinearLayout>
			<GridLayout
			android:id="@+id/cell_gridLayout_2"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_row="6"
			android:layout_column="0"
			android:background="#464749"
			android:rowCount="1"
			android:columnCount="2"
			>
				<LinearLayout
				android:id="@+id/cell_linearLayout_9"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_row="0"
				android:layout_column="0"
				android:background="#464749"
				android:orientation="vertical"
				android:layout_columnWeight="300.0"
				android:gravity="center_vertical"
				>
					<ImageView
					android:id="@+id/imageView_4"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="@drawable/disenoboletin19febrero05_1"
					>
					</ImageView>
					
				</LinearLayout>
				<LinearLayout
				android:id="@+id/cell_linearLayout_10"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_row="0"
				android:layout_column="1"
				android:background="#464749"
				android:orientation="vertical"
				android:layout_columnWeight="300.0"
				android:gravity="top"
				>
					<Button
					android:id="@+id/button_1"
					android:text="PRESENCIAL"
					android:textColor="#FFFFFF"
					android:textAlignment="gravity"
					android:textStyle="bold"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="left"
					android:background="@drawable/selector_on_click_view_orange"
					android:onClick="onClick"
					android:clickable="true"
					>
					</Button>
					<TextView
					android:id="@+id/textView_4"
					android:text="25 de Febrero 2016 - 2:00 pm"
					android:textColor="#FFFFFF"
					android:textAlignment="gravity"
					android:textStyle="italic"
					android:textAppearance="?android:attr/textAppearanceSmall"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="left"
					>
					</TextView>
					<Button
					android:id="@+id/button_2"
					android:text="VIRTUAL"
					android:textColor="#FFFFFF"
					android:textAlignment="gravity"
					android:textStyle="bold"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="left"
					android:background="@drawable/selector_on_click_view_orange"
					android:onClick="onClick"
					android:clickable="true"
					>
					</Button>
					<TextView
					android:id="@+id/textView_5"
					android:text="3 de Marzo 2016 - 2:00 pm"
					android:textColor="#FFFFFF"
					android:textAlignment="gravity"
					android:textStyle="italic"
					android:textAppearance="?android:attr/textAppearanceSmall"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="left"
					>
					</TextView>
					
				</LinearLayout>
				
			</GridLayout>
			<LinearLayout
			android:id="@+id/cell_linearLayout_11"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_row="7"
			android:layout_column="0"
			android:background="#FFFFFF"
			android:orientation="vertical"
			android:gravity="top"
			>
				<ImageView
				android:id="@+id/imageView_5"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:src="@drawable/disenoboletin19febrero14"
				android:onClick="onClick"
				android:clickable="true"
				>
				</ImageView>
				
			</LinearLayout>
			<GridLayout
			android:id="@+id/cell_gridLayout_3"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_row="8"
			android:layout_column="0"
			android:background="#C0C0C0"
			android:rowCount="1"
			android:columnCount="2"
			>
				<LinearLayout
				android:id="@+id/cell_linearLayout_12"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_row="0"
				android:layout_column="0"
				android:background="#C0C0C0"
				android:orientation="vertical"
				android:layout_columnWeight="300.0"
				android:gravity="top"
				>
					<LinearLayout
					android:id="@+id/video_linearLayout_13"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="@drawable/selector_on_click_video_orange"
					android:orientation="vertical"
					android:onClick="onClick"
					android:clickable="true"
					>
						<ImageView
						android:id="@+id/imageView_6"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:src="@drawable/disenoboletin19febrerob20"
						>
						</ImageView>
						<TextView
						android:id="@+id/textView_6"
						android:text="En el 2016 habra 1600 millones de cosas conectadas"
						android:textColor="#FFFFFF"
						android:textAlignment="center"
						android:textAppearance="?android:attr/textAppearanceSmall"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_gravity="center"
						>
						</TextView>
						<Button
						android:id="@+id/button_3"
						android:text="VER VIDEO"
						android:textColor="#FFFFFF"
						android:textAlignment="gravity"
						android:textStyle="bold"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_gravity="center"
						android:background="@drawable/selector_on_click_view_orange"
						android:onClick="onClick"
						android:clickable="true"
						>
						</Button>
						
					</LinearLayout>
					
				</LinearLayout>
				<LinearLayout
				android:id="@+id/cell_linearLayout_14"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_row="0"
				android:layout_column="1"
				android:background="#C0C0C0"
				android:orientation="vertical"
				android:layout_columnWeight="300.0"
				android:gravity="top"
				>
					<LinearLayout
					android:id="@+id/video_linearLayout_15"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="@drawable/selector_on_click_video_orange"
					android:orientation="vertical"
					android:onClick="onClick"
					android:clickable="true"
					>
						<ImageView
						android:id="@+id/imageView_7"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:src="@drawable/disenoboletin19febrerob17"
						android:onClick="onClick"
						android:clickable="true"
						>
						</ImageView>
						<TextView
						android:id="@+id/textView_7"
						android:text="Tendencias claves para un ano cargado de tecnologia"
						android:textColor="#FFFFFF"
						android:textAlignment="center"
						android:textAppearance="?android:attr/textAppearanceSmall"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_gravity="center"
						>
						</TextView>
						<Button
						android:id="@+id/button_4"
						android:text="VER VIDEO"
						android:textColor="#FFFFFF"
						android:textAlignment="gravity"
						android:textStyle="bold"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_gravity="center"
						android:background="@drawable/selector_on_click_view_orange"
						android:onClick="onClick"
						android:clickable="true"
						>
						</Button>
						
					</LinearLayout>
					
				</LinearLayout>
				
			</GridLayout>
			<LinearLayout
			android:id="@+id/cell_linearLayout_16"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_row="9"
			android:layout_column="0"
			android:background="#FFFFFF"
			android:orientation="vertical"
			android:gravity="top"
			>
				<LinearLayout
				android:id="@+id/divider_linearLayout_17"
				android:layout_width="match_parent"
				android:layout_height="10dp"
				android:paddingLeft="10dp"
				android:paddingRight="10dp"
				android:orientation="vertical"
				android:gravity="center"
				>
					<LinearLayout
					android:id="@+id/separator_linearLayout_18"
					android:layout_width="match_parent"
					android:layout_height="2dp"
					android:background="#C0C0C0"
					android:orientation="horizontal"
					>
						
					</LinearLayout>
					
				</LinearLayout>
				
			</LinearLayout>
			<LinearLayout
			android:id="@+id/cell_linearLayout_19"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_row="10"
			android:layout_column="0"
			android:background="#FFFFFF"
			android:orientation="vertical"
			android:gravity="top"
			>
				<ImageView
				android:id="@+id/imageView_8"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:src="@drawable/disenoboletin19febreropartners"
				>
				</ImageView>
				
			</LinearLayout>
			
		</GridLayout>
		
	</ScrollView>
	
</RelativeLayout>
