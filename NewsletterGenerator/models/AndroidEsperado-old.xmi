            <views xsi:type="view:GridLayout" layout_height="wrap_content" layout_row="5" background="#464749" rowCount="1" columnCount="2">
              <views xsi:type="view:ImageView" id="@+id/imageView_disenoboletin19febrero05_1" textAlignment="" layout_width="wrap_content" layout_height="wrap_content" paddingLeft="" src="@drawable/disenoboletin19febrero05_1"/>
              <views xsi:type="view:LinearLayout" layout_height="wrap_content" layout_column="1">
                <views xsi:type="view:Button" id="@+id/button" text="Presencial" layout_width="wrap_content" layout_height="wrap_content" background="#DE8616"/>
                <views xsi:type="view:TextView" id="@+id/textView8" text="25 de Febrero 2016 - 2:00 pm" textAppearance="?android:attr/textAppearanceSmall" layout_width="wrap_content" layout_height="wrap_content" background="#ffffff"/>
