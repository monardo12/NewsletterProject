            </views>
            <views xsi:type="view:GridLayout" layout_height="wrap_content" layout_row="7" background="#E5E5E5" rowCount="1" columnCount="2">
              <views xsi:type="view:LinearLayout" layout_width="0dp" layout_height="wrap_content" layout_weight="0.5" layout_gravity="">
                <views xsi:type="view:ImageView" id="@+id/imageView_disenoboletin19febrerob20" layout_width="wrap_content" layout_height="wrap_content" src="@drawable/disenoboletin19febrerob20"/>
                <views xsi:type="view:TextView" id="@+id/textView6" text="En el 2016 habr&#xe1; 1600 millones de cosas conectadas " textAlignment="center" textAppearance="?android:attr/textAppearanceSmall" layout_width="wrap_content" layout_height="wrap_content" background=""/>
              </views>
              <views xsi:type="view:LinearLayout" layout_width="0dp" layout_height="wrap_content" layout_weight="0.5" layout_column="1">
                <views xsi:type="view:ImageView" id="@+id/imageView_disenoboletin19febrerob17" layout_width="wrap_content" layout_height="wrap_content" src="@drawable/disenoboletin19febrerob17"/>
                <views xsi:type="view:TextView" id="@+id/textView7" text="Tendencias claves para un a&#xf1;o cargado de tecnolog&#xed;a" textAlignment="center" textAppearance="?android:attr/textAppearanceSmall" layout_width="wrap_content" layout_height="wrap_content"/>
