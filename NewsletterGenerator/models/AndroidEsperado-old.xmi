<?xml version="1.0" encoding="ASCII"?>
<android:Application xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:android="android" xmlns:view="view" xsi:schemaLocation="android view">
